pipeline{
  agent any
  
  stages{
  
    stage('init'){
      steps{
        echo 'this is init stage' 
    }
  }
   
    stage('compile'){
        steps{
          echo 'this is compile stage'
      }
     }
    
     stage('build'){
        steps{
          echo 'this is build stage'
      }
     }
    
   stage('test'){
      steps{
        echo 'this is test stage'
      }
     }
    
     stage('deploy'){
      steps{
       echo 'this is deploy stage'
      }
     }
   
}
}
